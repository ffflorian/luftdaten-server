<h3 class="s-title">Last 20 entries</h2>
  <table class="table table-striped table-scroll table-hover" id="entries">
    <thead>
      <tr></tr>
    </thead>
    <tbody>

    </tbody>
  </table>

  <script type="text/javascript">
    const thead = document.querySelectorAll('#entries thead tr')[0];
    const tbody = document.querySelectorAll('#entries tbody')[0];

    fetch('/data/latest/?limit=20').then(response => response.json()).then(entries => {
      Object.keys(entries[0]).forEach(key => {
        const strong = document.createElement('strong');
        strong.innerHTML = key;

        const td = document.createElement('td')
        td.appendChild(strong);

        thead.appendChild(td)
      })

      entries.forEach(entry => {
        const tr = document.createElement('tr');
        Object.values(entry).forEach(value => {
          const td = document.createElement('td')
          td.innerHTML = value;
          tr.appendChild(td);
        })
        tbody.appendChild(tr);
      })
    })
  </script>
