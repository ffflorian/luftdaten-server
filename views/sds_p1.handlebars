<h2>Fine dust 10 µm</h2>
<div id="chart_sds_p1" style="width: 900px; height: 500px"></div>

<script type="text/javascript">
  fetch('/data/sds_p1/?limit=300').then(response => response.json()).then(jsonData => {
    const sdsP1Data = jsonData.map(entry => {
      const {created_at, SDS_P1} = entry;
      return [new Date(created_at), SDS_P1];
    });

    function drawChart() {
      buildChart('chart_sds_p1', 'Fine dust 10 µm in the last 12 hours', 'Fine dust 10 µm', {format: '##,## µg/m3', viewWindow: {max: 30}}, sdsP1Data);
    }

    google.charts.load('current', {packages: ['line', 'corechart']});
    google.charts.setOnLoadCallback(drawChart);
  });
</script>
