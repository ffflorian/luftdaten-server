{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "dependencies": {
    "@types/swagger-schema-official": "2.0.21",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "date-fns": "2.16.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fs-extra": "9.0.1",
    "http-status-codes": "2.1.4",
    "knex": "0.21.15",
    "logdown": "3.3.1",
    "pm2": "4.5.1",
    "sqlite3": "5.0.1",
    "swagger-ui-express": "4.1.6"
  },
  "description": "Receive and serve data from your luftdaten device",
  "devDependencies": {
    "@ffflorian/eslint-config": "0.6.2",
    "@ffflorian/prettier-config": "0.1.2",
    "@types/body-parser": "1.19.0",
    "@types/compression": "1.7.0",
    "@types/express": "4.17.11",
    "@types/fs-extra": "9.0.6",
    "@types/node": "~14",
    "@types/swagger-ui-express": "4.1.2",
    "@typescript-eslint/eslint-plugin": "4.0.0",
    "@typescript-eslint/parser": "3.10.1",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jasmine": "4.1.1",
    "eslint-plugin-jsdoc": "31.0.7",
    "eslint-plugin-no-unsanitized": "3.1.4",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-typescript-sort-keys": "1.5.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "swagger-markdown": "1.4.3",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "luftdaten",
    "server",
    "typescript"
  ],
  "license": "GPL-3.0",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "luftdaten-server",
  "prettier": "@ffflorian/prettier-config",
  "repository": "https://github.com/ffflorian/luftdaten-server.git",
  "scripts": {
    "build": "yarn build:node && yarn build:docs",
    "build:commit": "git rev-parse HEAD > dist/commit",
    "build:node": "tsc",
    "build:docs": "node bin/swagger_docs.js",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build && yarn build:commit",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "eslint --ignore-path .gitignore --ext .ts src/",
    "prettier": "prettier \"*.{json,md}\"",
    "start": "pm2 start",
    "start:dev": "yarn build:commit && cross-env NODE_DEBUG='luftdaten-server/*' ENVIRONMENT='dev' ts-node src/index.ts",
    "test": "exit 0"
  },
  "version": "1.0.0"
}
