{
  "author": "Florian Keller <github@floriankeller.de>",
  "dependencies": {
    "@types/swagger-schema-official": "2.0.20",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cross-env": "6.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fs-extra": "8.1.0",
    "knex": "0.20.8",
    "logdown": "3.3.0",
    "moment": "2.24.0",
    "pm2": "4.2.3",
    "sqlite3": "4.1.1",
    "swagger-ui-express": "4.1.3"
  },
  "description": "Receive and serve data from your luftdaten device",
  "devDependencies": {
    "@ffflorian/prettier-config": "0.0.6",
    "@ffflorian/tslint-config": "0.7.1",
    "@types/body-parser": "1.17.1",
    "@types/compression": "1.0.1",
    "@types/express": "4.17.2",
    "@types/fs-extra": "8.0.1",
    "@types/node": "~12",
    "@types/swagger-ui-express": "4.1.1",
    "husky": "4.2.1",
    "lint-staged": "10.0.3",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "swagger-markdown": "1.2.0",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.1.0",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "luftdaten",
    "server",
    "typescript"
  ],
  "license": "GPL-3.0",
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "src/**/*.ts": [
      "tslint --project tsconfig.json --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "luftdaten-server",
  "prettier": "@ffflorian/prettier-config",
  "repository": "https://github.com/ffflorian/lufdaten-server.git",
  "scripts": {
    "build": "yarn build:node && yarn build:docs",
    "build:node": "tsc",
    "build:docs": "node bin/swagger_docs.js",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build && node bin/commit_hash.js",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --project tsconfig.json",
    "prettier": "prettier \"*.{json,md}\"",
    "start": "pm2 start",
    "start:dev": "cross-env NODE_DEBUG='luftdaten-server/*' ENVIRONMENT='dev' ts-node src/index.ts",
    "test": "exit 0"
  },
  "version": "1.0.0"
}
